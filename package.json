{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel 7",
    "starter",
    "config"
  ],
  "author": "amazingrv",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run clean && npm run server:dev",
    "server:dev": "node server.dev.js",
    "build": "npm run clean && npm run compile:app:prod && npm run prod:deps",
    "prod:deps": "cd ./dist && npm install --only=prod && cd ..",
    "compile:app:prod": "webpack --mode production --config webpack.prod.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "core-js": "^3.6.5",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "helmet": "^4.0.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.7",
    "sanitize.css": "^11.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-unicorn": "^21.0.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.10.0",
