{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "react starter with exposed config",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel 7",
    "starter",
    "config"
  ],
  "author": "amazingrv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run build:dev && npm run server",
    "server": "node ./dist/server.js",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run compile:server:prod && npm run compile:app:prod",
    "build:dev": "npm run clean && npm run compile:server:dev",
    "compile:server:dev": "webpack --mode development --config webpack.server.js",
    "compile:server:prod": "webpack --mode production --config webpack.server.js",
    "compile:app:dev": "webpack --mode development --config webpack.dev.js",
    "compile:app:prod": "webpack --mode production --config webpack.prod.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-unicorn": "^20.1.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "simple-progress-webpack-plugin": "^1.1.2",
    "terser-webpack-plugin": "^3.0.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
